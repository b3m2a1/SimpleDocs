Cell[BoxData[{
 RowBox[{
  RowBox[{"GitHubCheck", "[", "res_", "]"}], ":=", "\n", "\t", 
  RowBox[{"Which", "[", "\n", "\t\t", 
   RowBox[{
    RowBox[{
     RowBox[{"Head", "[", "res", "]"}], "===", "GitHub"}], ",", "\n", 
    "\t\t\t", 
    RowBox[{
     RowBox[{"Message", "[", 
      RowBox[{"Ems", "::", "badhub"}], "]"}], ";", "\n", "\t\t\t", 
     "$Failed"}], ",", "\n", "\t\t", 
    RowBox[{"!", 
     RowBox[{"AssociationQ", "@", "res"}]}], ",", "\n", "\t\t\t", 
    RowBox[{"res", ";", "\n", "\t\t\t", 
     RowBox[{"Message", "[", 
      RowBox[{"Ems", "::", "nohub"}], "]"}], ";", "\n", "\t\t\t", "$Failed"}],
     ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"res", "[", "\"\<StatusCode\>\"", "]"}], "===", "401"}], ",", 
    "\n", "\t\t\t", 
    RowBox[{
     RowBox[{"Message", "[", 
      RowBox[{"Ems", "::", "noauth"}], "]"}], ";", "\n", "\t\t\t", 
     "$Failed"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"res", "[", "\"\<StatusCode\>\"", "]"}], "===", "404"}], ",", 
    "\n", "\t\t\t", 
    RowBox[{
     RowBox[{"Message", "[", 
      RowBox[{
       RowBox[{"Ems", "::", "badend"}], ",", " ", 
       RowBox[{"res", "[", "\"\<Message\>\"", "]"}]}], "]"}], ";", "\n", 
     "\t\t\t", "$Failed"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"res", "[", "\"\<StatusCode\>\"", "]"}], "===", "409"}], ",", 
    "\n", "\t\t\t", 
    RowBox[{
     RowBox[{"Message", "[", 
      RowBox[{
       RowBox[{"Ems", "::", "badcon"}], ",", " ", 
       RowBox[{"res", "[", "\"\<Message\>\"", "]"}]}], "]"}], ";", "\n", 
     "\t\t\t", "$Failed"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"res", "[", "\"\<StatusCode\>\"", "]"}], ">", "400"}], ",", "\n",
     "\t\t\t", 
    RowBox[{
     RowBox[{"Message", "[", 
