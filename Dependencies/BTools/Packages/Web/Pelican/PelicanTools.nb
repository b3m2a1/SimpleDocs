TIMEZONE = '`Timezone`'\n\nDEFAULT_LANG = u'`Language`'\n\n# Feed generation \
is usually not desired when developing\nFEED_ALL_ATOM = None\n\
CATEGORY_FEED_ATOM = None\nTRANSLATION_FEED_ATOM = None\nAUTHOR_FEED_ATOM = \
None\nAUTHOR_FEED_RSS = None\n\n# Enable Markdown\nMARKDOWN = {\n\t\
`MarkdownExtensions`\n\t}\n\n# Theme\n\nTHEME = '`Theme`'\n\n# Blogroll\n\
LINKS = `Links`\n\n# Social widget\nSOCIAL = `Social`\n\nDEFAULT_PAGINATION = \
`PaginationNumber`\n\n# Uncomment following line if you want \
document-relative URLs when developing\n#RELATIVE_URLS = \
`UseRelativeURLs`\>\""}], ";"}]], "CodeInput",
