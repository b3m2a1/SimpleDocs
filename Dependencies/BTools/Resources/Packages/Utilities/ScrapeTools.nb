Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"fileGrepMultiLine", "[", 
    RowBox[{"file_", ",", "pat_", ",", "containsQ_"}], "]"}], ":=", "\n", 
   "\t", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"text", "=", 
       RowBox[{"fastTextImport", "@", "file"}]}], "}"}], ",", "\n", "\t\t", 
     RowBox[{"If", "[", 
      RowBox[{"containsQ", ",", "\n", "\t\t\t", 
       RowBox[{"StringContainsQ", "[", 
        RowBox[{"text", ",", "pat"}], "]"}], ",", "\n", "\t\t\t", 
       RowBox[{"Association", "@", "\n", "\t\t\t\t", 
        RowBox[{"Table", "[", "\n", "\t\t\t\t\t", 
         RowBox[{
          RowBox[{"With", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"chunks", "=", "\n", "\t", "\t\t\t\t\t", 
              RowBox[{"StringTake", "[", 
               RowBox[{"text", ",", 
                RowBox[{"{", "\n", "\t", "\t\t\t\t\t\t", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"1", ",", 
                    RowBox[{"First", "@", "sp"}]}], "}"}], ",", "\n", "\t", 
                  "\t\t\t\t\t\t", "sp"}], "\n", "\t", "\t\t\t\t\t\t", "}"}]}],
                "]"}]}], "}"}], ",", "\n", "\t\t\t\t\t\t", 
            RowBox[{"With", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"splits", "=", 
                RowBox[{
                 RowBox[{
                  RowBox[{"StringSplit", "[", 
                   RowBox[{"#", ",", "\"\<\\n\>\""}], "]"}], "&"}], "/@", 
                 "chunks"}]}], "}"}], ",", "\n", "\t\t\t\t\t\t\t", 
              RowBox[{
               RowBox[{"{", "\n", "\t\t\t\t\t\t\t\t", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"Length", "@", 
                    RowBox[{"First", "@", "splits"}]}], ",", 
                   RowBox[{"StringLength", "@", 
                    RowBox[{"First", "@", 
                    RowBox[{"Last", "@", "splits"}]}]}]}], "}"}], ",", "\n", 
                 "\t\t\t\t\t\t\t\t", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"Length", "@", 
                    RowBox[{"First", "@", "splits"}]}], "+", "\n", 
                    "\t\t\t\t\t\t\t\t\t", 
                    RowBox[{"(", 
                    RowBox[{"Length", "@", 
                    RowBox[{"Last", "@", "splits"}]}], ")"}], "-", "1"}], ",",
                    "\n", "\t\t\t\t\t\t\t\t\t\t", 
                   RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"Length", "@", 
                    RowBox[{"Length", "@", 
                    RowBox[{"Last", "@", "splits"}]}]}], ">", "1"}], ",", 
                    "\n", "\t\t\t\t\t\t\t\t\t\t\t", "0", ",", "\n", 
                    "\t\t\t\t\t\t\t\t\t\t\t", 
                    RowBox[{"StringLength", "@", 
                    RowBox[{"First", "@", 
                    RowBox[{"Last", "@", "splits"}]}]}]}], "\n", 
                    "\t\t\t\t\t\t\t\t\t\t\t", "]"}], "+", "\n", 
                    "\t\t\t\t\t\t\t\t\t\t\t", 
                    RowBox[{"StringLength", "@", 
                    RowBox[{"Last", "@", 
                    RowBox[{"Last", "@", "splits"}]}]}]}]}], "}"}]}], "\n", 
                "\t\t\t\t\t\t\t\t", "}"}], "->", "\n", "\t\t\t\t\t\t\t\t\t", 
               RowBox[{"Last", "@", "chunks"}]}]}], "\n", "\t\t\t\t\t\t\t", 
             "]"}]}], "\n", "\t\t\t\t\t\t", "]"}], ",", "\n", "\t\t\t\t\t", 
          RowBox[{"{", 
           RowBox[{"sp", ",", 
            RowBox[{"StringPosition", "[", 
             RowBox[{"text", ",", "pat", ",", 
              RowBox[{"Overlaps", "\[Rule]", "False"}]}], "]"}]}], "}"}]}], 
         "\n", "\t\t\t\t\t", "]"}]}]}], "\n", "\t\t\t", "]"}]}], "\n", "\t\t",
     "]"}]}], ";"}]], "CodeInput",
 CellChangeTimes->{{3.6979796891406527`*^9, 3.697980027292883*^9}, {
