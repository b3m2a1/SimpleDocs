Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"fileGrepSingleLine", "[", 
    RowBox[{"file_", ",", "pat_", ",", "containsQ_"}], "]"}], ":=", "\n", 
   "\t", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", "\n", "\t\t", 
      RowBox[{
       RowBox[{"fileGrepOpened", "=", 
        RowBox[{"OpenRead", "[", "file", "]"}]}], ",", 
       RowBox[{"fileGrepChunkSize", "=", "100000"}], ",", "\n", "\t\t", 
       "fileGrepChunk", ",", "fileGrepFlag"}], "}"}], ",", "\n", "\t\t", 
     RowBox[{
      RowBox[{"fileGrepChunk", "=", "\n", "\t\t\t", 
       RowBox[{"ReadList", "[", 
        RowBox[{"fileGrepOpened", ",", "String", ",", "fileGrepChunkSize"}], 
        "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"fileGrepFlag", ":=", "\n", "\t\t\t", 
       RowBox[{
        RowBox[{"Length", "@", "fileGrepChunk"}], ">", "0"}]}], ";", "\n", 
      "\t\t", 
      RowBox[{
       RowBox[{"Reap", "[", "\n", "\t\t\t", 
        RowBox[{
         RowBox[{"While", "[", 
          RowBox[{"fileGrepFlag", ",", "\n", "\t", "\t\t\t", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{"containsQ", ",", "\n", "\t", "\t\t\t\t", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"Or", "@@", 
                 RowBox[{"StringContainsQ", "[", 
                  RowBox[{"fileGrepChunk", ",", "pat"}], "]"}]}], ",", "\n", 
                "\t", "\t\t\t\t\t", 
                RowBox[{
                 RowBox[{"Sow", "@", "True"}], ";", "\n", "\t", "\t\t\t\t\t", 
                 
                 RowBox[{"fileGrepFlag", "=", "False"}]}]}], "\n", "\t", 
               "\t\t\t\t\t", "]"}], ",", "\n", "\t", "\t\t\t\t", 
              RowBox[{"Sow", "@", "\n", "\t", "\t\t\t\t\t", 
               RowBox[{"StringCases", "[", 
                RowBox[{"fileGrepChunk", ",", "pat"}], "]"}]}]}], "\n", 
             "\t\t\t\t\t", "]"}], ";", "\n", "\t\t\t\t", 
            RowBox[{"fileGrepChunk", "=", "\n", "\t\t\t\t\t", 
             RowBox[{"ReadList", "[", 
              RowBox[{
              "fileGrepOpened", ",", "String", ",", "fileGrepChunkSize", ",", 
               "\n", "\t\t\t\t\t\t", 
               RowBox[{"NullRecords", "\[Rule]", "True"}]}], "]"}]}], ";"}]}],
           "\n", "\t\t\t\t", "]"}], ";", "\n", "\t\t\t\t", 
         RowBox[{"Close", "@", "fileGrepOpened"}], ";"}], "\n", "\t\t\t", 
        "]"}], "//", 
       RowBox[{"If", "[", 
        RowBox[{"containsQ", ",", "\n", "\t\t\t\t\t", 
         RowBox[{"Replace", "[", 
          RowBox[{"{", "\n", "\t\t\t\t\t\t", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{"_", ",", 
               RowBox[{"{", "}"}]}], "}"}], "->", "False"}], ",", "\n", 
            "\t\t\t\t\t\t", 
            RowBox[{"_", "\[Rule]", "True"}]}], "\n", "\t\t\t\t\t\t", "}"}], 
          "]"}], ",", "\n", "\t\t\t\t\t", 
         RowBox[{"Replace", "[", 
          RowBox[{"{", "\n", "\t\t\t\t\t\t", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{"_", ",", 
               RowBox[{"{", "r_", "}"}]}], "}"}], ":>", "\n", 
             "\t\t\t\t\t\t\t", 
             RowBox[{"Association", "@", 
              RowBox[{"DeleteCases", "[", "\n", "\t\t\t\t\t\t\t\t", 
               RowBox[{
                RowBox[{"MapIndexed", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{"First", "@", "#2"}], "\[Rule]", "#"}], "&"}], 
                  ",", 
                  RowBox[{"Join", "@@", "r"}]}], "]"}], ",", "\n", 
                "\t\t\t\t\t\t\t\t", 
                RowBox[{"_", "\[Rule]", 
                 RowBox[{"{", "}"}]}]}], "\n", "\t\t\t\t\t\t\t\t", "]"}]}]}], 
            ",", "\n", "\t\t\t\t\t\t", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"_", ",", 
               RowBox[{"{", "}"}]}], "}"}], ":>", "\n", "\t\t\t\t\t\t\t", 
             RowBox[{"<|", "|>"}]}]}], "\n", "\t\t\t\t\t\t", "}"}], "]"}]}], 
        "\n", "\t\t\t\t\t", "]"}]}]}]}], "\n", "\t\t", "]"}]}], 
  ";"}]], "CodeInput",
 CellChangeTimes->{{3.6979796891406527`*^9, 3.697980027292883*^9}, {
   3.697980120616947*^9, 3.697980121576089*^9}, {3.697980163184157*^9, 
