    "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FileGrep", "[", 
    RowBox[{
     RowBox[{"file", ":", 
      RowBox[{"filePattern", "?", 
       RowBox[{"(", 
        RowBox[{"Not", "@*", "DirectoryQ"}], ")"}]}]}], ",", "pat_", ",", 
     RowBox[{"ops", ":", 
      RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", "\n", "\t", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"function", "=", "\n", "\t\t", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"OptionValue", "[", "\"\<MultilineMatch\>\"", "]"}], "//", 
          "TrueQ"}], ",", "\n", "\t\t\t", "fileGrepMultiLine", ",", "\n", 
